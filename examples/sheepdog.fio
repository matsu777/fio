#####
#expected form of target:
#unix:<path_of_unix_domain_socket>:<vdi_name>
#tcp:<sheepdog_host>:<sheepdog_port>:<vdi>
#e.g. target=tcp:127.0.0.1:7000:testvdi
#####
[global]
ioengine=sheepdog
target=tcp:localhost:7000:testvdi
kb_base=1024
direct=1
invalidate=1
group_reporting

[sheepdog_test]
rw=read
size=1000M
numjobs=8
bs=4k
